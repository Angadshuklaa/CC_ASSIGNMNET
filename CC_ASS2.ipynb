      "metadata": {
        "id": "3MuR3Lvm9PuZ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "L = [10, 20, 30, 40, 50, 60, 70, 80]\n",
        "\n",
        "L.extend([200, 300])\n",
        "\n",
        "L.remove(10)\n",
        "L.remove(30)\n",
        "\n",
        "L.sort()\n",
        "\n",
        "L.sort(reverse=True)\n",
        "\n",
        "print(L)"
